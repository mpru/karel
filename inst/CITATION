citHeader("To cite karel in publications use:")

bibentry(
  key = "karelpackage",
  bibtype    = "Misc",
  title    = "karel: Learning programming with Karel the robot",
  author   = person("Marcos", "Prunello"),
  year     = 2024,
  url      = "https://mpru.github.io/karel/",
  textVersion = paste("Prunello M (2024). karel: Learning programming with Karel the robot. R package version 0.1.1.9000, <https://mpru.github.io/karel/>.")
)
